[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = true
[core]
  excludesfile = /home/mlopes/.gitignore_global
[diff]
  mnemonicprefix = true
[difftool "sourcetree"]
  cmd = opendiff \"$LOCAL\" \"$REMOTE\"
  path = 
[mergetool "sourcetree"]
  trustExitCode = true
[mergetool "kdiff3"]
  trustExitCode = false
[merge]
  tool = extMerge
[mergetool "extMerge"]
  cmd = /usr/local/bin/extMerge \"$BASE\" \"$LOCAL\" \"$REMOTE\" -o \"$MERGED\"
[achievement]
  upload = true
[push]
  default = simple
[include]
  path = .gituser
  path = .gitaliases
[user]
  signingkey = 14D40CA1E1A806A015C4A06B372EC33EB388121A
[commit]
  gpgsign = true
